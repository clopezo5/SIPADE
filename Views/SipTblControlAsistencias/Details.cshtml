@model SIPADE.Models.SipTblControlAsistencia

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Control de Asistencia</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha Hora de Entrada
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblCasFechaHoraEntrada)
        </dd>
        <dt class = "col-sm-2">
            Fecha Hora de Salida
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblCasFechaHoraSalida)
        </dd>
        <dt class="col-sm-2">
            ID Empleado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblEmp.SipTblEmpId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.SipTblCasId">Editar</a> |
    <a asp-action="Index">Regresar a la lista</a>
</div>
