@model SIPADE.Models.SipTblEncabezadoPlanilla

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Encabezado de Planilla</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Fecha y hora
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblEplFechahora)
        </dd>
        <dt class="col-sm-2">
            Fecha de inicio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblEplFechainicio)
        </dd>
        <dt class="col-sm-2">
            Fecha de fin
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SipTblEplFechafin)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.SipTblEplId">Editar</a> |
    <a asp-action="Index">Regresar a la lista</a>
</div>
